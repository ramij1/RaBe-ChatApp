<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.redwan.firebasechatapp.AllUsersActivity">

    <include layout="@layout/all_toolbar" android:id="@+id/toolbarId_allUsers" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/all_Users_RecyclerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/toolbarId_allUsers">

    </android.support.v7.widget.RecyclerView>

</RelativeLayout>
